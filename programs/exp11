
ORG 0000h
    LJMP main
    ORG 0040h
    main: MOV R0,#03h                     
          MOV R1,#06h                    
          MOV R2,#01h                    
          MOV A,R0
          MOV B,R1
          CJNE A,B,label1                     
          MOV R4,A 
          SJMP stop
  label1: JNC loop               
          MOV A,R1
          MOV B,R0
          MOV R0,A
          MOV R1,B
  loop:   MOV A,R2       
          MOV B,R0
          CJNE A,B,next  
          SJMP stop
  next:   MOV A,R1       
          MOV B,R2
          MUL AB
          MOV R4,A       
          INC R2
          MOV B,R0
          DIV AB          
          MOV R3,B
          CJNE R3,#00h,loop 
          SJMP stop
  stop:   JMP stop
END
